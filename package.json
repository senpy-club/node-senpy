{
  "name": "senpy",
  "version": "0.1.0",
  "description": "The Senpy Club API Wrapper for Node.js",
  "keywords": [
    "typescript",
    "node"
  ],
  "homepage": "https://npmjs.org/package/senpy",
  "bugs": {
    "email": "contact@fuwn.me",
    "url": "https://github.com/senpy-club/node-senpy/issues"
  },
  "license": "GPL-3.0-only",
