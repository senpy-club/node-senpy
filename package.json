    "@types/node": "17.0.1",
    "esbuild": "0.14.27",
    "eslint": "8.57.0",
    "jest": "27.5.1",
    "pinst": "3.0.0",
    "prettier": "2.6.0",
    "ts-jest": "^27.1.2",
    "typescript": "4.6.2"
  },
  "exports": {
    "./": "./dist/index.js"
  },
  "type": "module",
  "types": "./dist/index.d.ts"
